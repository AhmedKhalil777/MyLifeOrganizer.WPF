<Window x:Class="MyOrganizer.WPF.Pages.Certificates"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyOrganizer.WPF.Pages"
        mc:Ignorable="d"
        Title="Certificates" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="315" Margin="167,41,0,0" Source="https://github.com/AhmedKhalil777/Hall-of-FAME-and-SHAME/blob/master/Cert/GP.jpeg?raw=true" VerticalAlignment="Top" Width="479" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>

    </Grid>
</Window>
